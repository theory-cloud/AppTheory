{
  "Mappings": {
    "AWSCloudFrontPartitionHostedZoneIdMap": {
      "aws": {
        "zoneId": "Z2FDTNDATAQYW2"
      },
      "aws-cn": {
        "zoneId": "Z3RFFRIM2A3IF5"
      }
    }
  },
  "Parameters": {
    "BootstrapVersion": {
      "Default": "/cdk-bootstrap/hnb659fds/version",
      "Description": "Version of the CDK Bootstrap resources in this environment, automatically retrieved from SSM Parameter Store. [cdk:skip]",
      "Type": "AWS::SSM::Parameter::Value<String>"
    }
  },
  "Resources": {
    "CertCertificate2FA294AA": {
      "Properties": {
        "CertificateTransparencyLoggingPreference": "ENABLED",
        "DomainName": "media.example.com",
        "DomainValidationOptions": [
          {
            "DomainName": "media.example.com",
            "HostedZoneId": {
              "Ref": "ZoneA5DE4B68"
            }
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "TestStack/Cert/Certificate"
          }
        ],
        "ValidationMethod": "DNS"
      },
      "Type": "AWS::CertificateManager::Certificate"
    },
    "MediaCdnAliasRecordAF16648C": {
      "Properties": {
        "AliasTarget": {
          "DNSName": {
            "Fn::GetAtt": [
              "MediaCdnDistribution23F4ABB7",
              "DomainName"
            ]
          },
          "HostedZoneId": {
            "Fn::FindInMap": [
              "AWSCloudFrontPartitionHostedZoneIdMap",
              {
                "Ref": "AWS::Partition"
              },
              "zoneId"
            ]
          }
        },
        "HostedZoneId": {
          "Ref": "ZoneA5DE4B68"
        },
        "Name": "media.example.com.",
        "Type": "A"
      },
      "Type": "AWS::Route53::RecordSet"
    },
    "MediaCdnDistribution23F4ABB7": {
      "Properties": {
        "DistributionConfig": {
          "Aliases": [
            "media.example.com"
          ],
          "Comment": "Media CDN with custom domain",
          "DefaultCacheBehavior": {
            "AllowedMethods": [
              "GET",
              "HEAD",
              "OPTIONS"
            ],
            "CachePolicyId": "658327ea-f89d-4fab-a63d-7e88639e58f6",
            "Compress": true,
            "TargetOriginId": "TestStackMediaCdnDistributionOrigin123AB81E4",
            "ViewerProtocolPolicy": "redirect-to-https"
          },
          "Enabled": true,
          "HttpVersion": "http2",
          "IPV6Enabled": true,
          "Origins": [
            {
              "DomainName": {
                "Fn::GetAtt": [
                  "MediaCdnMediaBucketF5806617",
                  "RegionalDomainName"
                ]
              },
              "Id": "TestStackMediaCdnDistributionOrigin123AB81E4",
              "OriginAccessControlId": {
                "Fn::GetAtt": [
                  "MediaCdnDistributionOrigin1S3OriginAccessControl2149847E",
                  "Id"
                ]
              },
              "S3OriginConfig": {
                "OriginAccessIdentity": ""
              }
            }
          ],
          "ViewerCertificate": {
            "AcmCertificateArn": {
              "Ref": "CertCertificate2FA294AA"
            },
            "MinimumProtocolVersion": "TLSv1.2_2021",
            "SslSupportMethod": "sni-only"
          }
        }
      },
      "Type": "AWS::CloudFront::Distribution"
    },
    "MediaCdnDistributionOrigin1S3OriginAccessControl2149847E": {
      "Properties": {
        "OriginAccessControlConfig": {
          "Name": "TestStackMediaCdnDistributioOrigin1S3OriginAccessControl197396EF",
          "OriginAccessControlOriginType": "s3",
          "SigningBehavior": "always",
          "SigningProtocol": "sigv4"
        }
      },
      "Type": "AWS::CloudFront::OriginAccessControl"
    },
    "MediaCdnMediaBucketF5806617": {
      "DeletionPolicy": "Retain",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        },
        "OwnershipControls": {
          "Rules": [
            {
              "ObjectOwnership": "BucketOwnerEnforced"
            }
          ]
        },
        "PublicAccessBlockConfiguration": {
          "BlockPublicAcls": true,
          "BlockPublicPolicy": true,
          "IgnorePublicAcls": true,
          "RestrictPublicBuckets": true
        }
      },
      "Type": "AWS::S3::Bucket",
      "UpdateReplacePolicy": "Retain"
    },
    "MediaCdnMediaBucketPolicy65D31F7A": {
      "Properties": {
        "Bucket": {
          "Ref": "MediaCdnMediaBucketF5806617"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": "false"
                }
              },
              "Effect": "Deny",
              "Principal": {
                "AWS": "*"
              },
              "Resource": [
                {
                  "Fn::GetAtt": [
                    "MediaCdnMediaBucketF5806617",
                    "Arn"
                  ]
                },
                {
                  "Fn::Join": [
                    "",
                    [
                      {
                        "Fn::GetAtt": [
                          "MediaCdnMediaBucketF5806617",
                          "Arn"
                        ]
                      },
                      "/*"
                    ]
                  ]
                }
              ]
            },
            {
              "Action": "s3:GetObject",
              "Condition": {
                "StringEquals": {
                  "AWS:SourceArn": {
                    "Fn::Join": [
                      "",
                      [
                        "arn:",
                        {
                          "Ref": "AWS::Partition"
                        },
                        ":cloudfront::",
                        {
                          "Ref": "AWS::AccountId"
                        },
                        ":distribution/",
                        {
                          "Ref": "MediaCdnDistribution23F4ABB7"
                        }
                      ]
                    ]
                  }
                }
              },
              "Effect": "Allow",
              "Principal": {
                "Service": "cloudfront.amazonaws.com"
              },
              "Resource": {
                "Fn::Join": [
                  "",
                  [
                    {
                      "Fn::GetAtt": [
                        "MediaCdnMediaBucketF5806617",
                        "Arn"
                      ]
                    },
                    "/*"
                  ]
                ]
              }
            }
          ],
          "Version": "2012-10-17"
        }
      },
      "Type": "AWS::S3::BucketPolicy"
    },
    "ZoneA5DE4B68": {
      "Properties": {
        "Name": "example.com."
      },
      "Type": "AWS::Route53::HostedZone"
    }
  },
  "Rules": {
    "CheckBootstrapVersion": {
      "Assertions": [
        {
          "Assert": {
            "Fn::Not": [
              {
                "Fn::Contains": [
                  [
                    "1",
                    "2",
                    "3",
                    "4",
                    "5"
                  ],
                  {
                    "Ref": "BootstrapVersion"
                  }
                ]
              }
            ]
          },
          "AssertDescription": "CDK bootstrap stack version 6 required. Please run 'cdk bootstrap' with a recent version of the CDK CLI."
        }
      ]
    }
  }
}
