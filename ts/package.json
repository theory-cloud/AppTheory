{
  "name": "@theory-cloud/apptheory",
  "version": "0.9.0-rc.2",
  "description": "AppTheory â€” multi-language serverless application framework (TypeScript package)",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --max-warnings=0 --report-unused-disable-directives \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.971.0",
    "@theory-cloud/tabletheory-ts": "https://github.com/theory-cloud/TableTheory/releases/download/v1.4.0/theory-cloud-tabletheory-ts-1.4.0.tgz"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.2.3",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "17.0.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24"
  }
}
