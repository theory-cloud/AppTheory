{
  "id": "m1.eventbridge.rule_routing",
  "tier": "m1",
  "name": "EventBridge: route by rule name extracted from resources ARN",
  "setup": {
    "eventbridge": [
      { "rule_name": "rule-a", "handler": "eventbridge_static_a" },
      { "rule_name": "rule-b", "handler": "eventbridge_static_b" }
    ]
  },
  "input": {
    "aws_event": {
      "source": "eventbridge",
      "event": {
        "version": "0",
        "id": "evt-1",
        "detail-type": "Scheduled Event",
        "source": "aws.events",
        "account": "000000000000",
        "time": "1970-01-01T00:00:00Z",
        "region": "us-east-1",
        "resources": ["arn:aws:events:us-east-1:000000000000:rule/rule-b"],
        "detail": {}
      }
    }
  },
  "expect": {
    "output_json": { "handler": "b" }
  }
}

