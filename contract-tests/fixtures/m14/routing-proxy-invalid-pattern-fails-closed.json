{
  "id": "m14.routing.proxy.invalid_pattern_fails_closed",
  "tier": "m14",
  "name": "Router: invalid {proxy+} patterns fail closed",
  "setup": {
    "routes": [
      { "method": "GET", "path": "/assets/{proxy+}/tail", "handler": "echo_path_params" }
    ]
  },
  "input": {
    "request": {
      "method": "GET",
      "path": "/assets/a/tail",
      "query": {},
      "headers": {},
      "body": { "encoding": "utf8", "value": "" },
      "is_base64": false
    }
  },
  "expect": {
    "response": {
      "status": 404,
      "headers": {
        "content-type": ["application/json; charset=utf-8"],
        "x-request-id": ["req_test_123"]
      },
      "cookies": [],
      "body_json": {
        "error": {
          "code": "app.not_found",
          "message": "not found",
          "request_id": "req_test_123"
        }
      },
      "is_base64": false
    }
  }
}

