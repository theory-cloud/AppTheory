{
  "id": "p0.adapter.lambda_function_url.binary_response_base64",
  "tier": "p0",
  "name": "Lambda Function URL: binary response bodies are base64 encoded",
  "setup": {
    "routes": [
      { "method": "GET", "path": "/binary", "handler": "binary_body" }
    ]
  },
  "input": {
    "request": {
      "method": "GET",
      "path": "/binary",
      "query": {},
      "headers": {},
      "body": { "encoding": "utf8", "value": "" },
      "is_base64": false
    },
    "aws_event": {
      "source": "lambda_function_url",
      "event": {
        "version": "2.0",
        "rawPath": "/binary",
        "rawQueryString": "",
        "cookies": [],
        "headers": {},
        "queryStringParameters": null,
        "requestContext": { "http": { "method": "GET", "path": "/binary" } },
        "body": "",
        "isBase64Encoded": false
      }
    }
  },
  "expect": {
    "response": {
      "status": 200,
      "headers": { "content-type": ["application/octet-stream"] },
      "cookies": [],
      "body": { "encoding": "base64", "value": "AAEC" },
      "is_base64": true
    }
  }
}

